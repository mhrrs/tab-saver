{
    "manifest_version": 3,
    "name": "interfaceTest",
    "description": "Test the GUI of the CaseBuilder",
    "version": "0.1",
    "content_scripts":[
      {
        "matches":["<all_urls>"],
        "html": ["content.html"],
        "css": ["content.css"],
        "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "content.html",
    "default_title": "DOM Reader"
  },
  "permissions": ["scripting","tabs"],
  "host_permissions":["<all_urls>"]
}
